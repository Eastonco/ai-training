# Environment variables for Docker Compose
# Copy this file to .env and modify the ports if needed

# Backend port (default: 5000)
BACKEND_PORT=5000

# Frontend port (default: 3000)
FRONTEND_PORT=3000

# CORS origins (automatically set based on FRONTEND_PORT)
# CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Example: If port 5000 is in use, change to:
# BACKEND_PORT=5001
# FRONTEND_PORT=3001
# CORS_ORIGINS will be automatically set to http://localhost:3001,http://127.0.0.1:3001
